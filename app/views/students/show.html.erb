<html>
    <head>
        <title></title>
    </head>
    <body>
        <h1><%= @dojo.branch %>'s <%= @student.first_name %> <%= @student.last_name %></h1>
        <p>Email: <%= @student.email %></p>
        <p>Cohort</p>
        <table>
            <tr>
                <td>First Name</td>
                <td>Last Name</td>
                <td>Email</td>
            </tr>
            <% @dojo.students.each do |student| %>
                <tr>
                    <td><%= student.first_name %></td>
                    <td><%= student.last_name %></td>
                    <td><%= student.email %></td>
                </tr>
            <% end %>
        </table>
        <p><a href='/dojos/<%= @dojo.id %>'>Home</a> | <a href='/dojos/<%= @dojo.id %>/students/<%= @student.id %>/edit'>Edit</a></p>
    </body>
</html>